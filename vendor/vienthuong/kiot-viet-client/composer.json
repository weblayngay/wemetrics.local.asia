{
    "name": "vienthuong/kiot-viet-client",
    "type": "library",
    "description": "A Small PHP Client package for working with KiotViet API",
    "keywords": [
        "vienthuong",
        "kiot-viet-client"
    ],
    "homepage": "https://github.com/vienthuong/kiot-viet-client",
    "license": "MIT",
    "authors": [
        {
            "name": "vienthuong",
            "email": "levienthuong@gmail.com",
            "homepage": "https://github.com/vienthuong",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symplify/easy-coding-standard": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "VienThuong\\KiotVietClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VienThuong\\KiotVietClient\\": "tests"
        }
    },
    "scripts": {
        "ecs": "vendor/bin/ecs check src",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
